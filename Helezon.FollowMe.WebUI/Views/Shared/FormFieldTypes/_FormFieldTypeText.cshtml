@model FollowMe.Web.Models.DynamicFormField
<div class="field-repeater">
    <div class="form-group">
        <div class="col-md-12">
            <div data-repeater-list="group-formfield">
                @{
                    //var answeredField = field.ChildDynamicFormFields.Where(x => Model.TermNameValues.Values.Select(a => a.DynamicFieldId).ToList().Contains(x.Id));
                    <div data-repeater-item>
                        <div class="form-group">
                            <div class="col-md-10 ">
                                @Html.Partial("_DynamicForm2", new FollowMe.Web.Models.ViewModels.DynamicFormViewModel() { DynamicFormFields = Model.ChildDynamicFormFields, TermNameValues = Model.TermNameValues }, new ViewDataDictionary { { "PanelHeader", Model.Label.Trim() } })
                            </div>
                            <div class="col-md-2">
                                <button class="btn btn-danger" type="button" data-repeater-delete>Delete</button>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-4">
            <button class="btn btn-default" type="button" data-repeater-create>Add</button>&nbsp;&nbsp;&nbsp;
            @*<button class="btn btn-primary" id="form_submit" type="submit">Save</button>*@
        </div>
    </div>
</div>
